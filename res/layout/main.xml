<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <fragment android:name="com.tickit.SplashActivity"
          android:id="@+id/splashActivity"
          android:layout_width="match_parent"
          android:layout_height="match_parent" />
    <fragment android:name="com.tickit.EmailLogin"
          android:id="@+id/emailLoginFragment"
          android:layout_width="match_parent"
          android:layout_height="match_parent" />
    <fragment android:name="com.tickit.EmailRegister"
          android:id="@+id/emailRegisterFragment"
          android:layout_width="match_parent"
          android:layout_height="match_parent" />
    
</LinearLayout>
